    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "aws-amplify": "^4.2.5",
    "aws-sdk": "^2.978.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "dotenv": "^10.0.0",
    "expo": "~42.0.1",
    "expo-application": "~3.2.0",
    "expo-constants": "~11.0.1",
