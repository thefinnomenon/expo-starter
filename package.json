{
  "main": "./index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "deploy:dev": "cd backend && NODE_ENV=development cdk deploy",
    "deploy:prod": "cd backend && NODE_ENV=production cdk deploy",
    "test": "jest",
    "test:dev": "jest --watch --changedSince=origin/master",
    "test:debug": "jest -o --watch",
    "test:updateSnapshots": "jest -u",
    "test:ci": "jest --coverage",
    "test:badges": "npm run test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test start http://localhost:3000 cypress:headless",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/netinfo": "^6.0.1",
    "aws-amplify": "^4.2.5",
    "aws-sdk": "^2.978.0",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "dotenv": "^10.0.0",
    "expo": "~42.0.1",
    "expo-application": "~3.2.0",
    "expo-constants": "~11.0.1",
    "expo-device": "~3.3.0",
    "expo-file-system": "~11.1.3",
    "expo-localization": "~10.2.0",
    "expo-location": "~12.1.2",
    "expo-random": "~11.2.0",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-logs": "^3.0.4",
    "react-native-web": "~0.13.12",
    "sentry-expo": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@expo/webpack-config": "^0.15.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "^17.0.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cypress": "^8.3.1",
    "dotenv-webpack": "^7.0.3",
    "jest-coverage-badges": "^1.1.2",
    "jest-expo": "^42.1.0",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.13.1",
    "ts-jest": "^26",
    "typescript": "~4.0.0"
  },
  "private": true
}
